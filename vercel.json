{
  "version": 2,
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "NODE_ENV": "production",
    "PORT": "3000"
  },
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ],
  "functions": {
    "src/index.js": {
      "memory": 3008,
      "maxDuration": 30
    }
  }
}
